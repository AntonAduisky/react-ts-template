{
    "name": "webpack-react-typescript-template",
    "version": "1.0.0",
    "description": "Template",
    "keywords": [
        "react",
        "typescript",
        "webpack",
        "react-router",
        "eslint",
        "prettier",
        "husky",
        "commitlint"
    ],
    "homepage": "https://github.com/AntonAduisky/webpack-react-typescript",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/AntonAduisky/webpack-react-typescript.git"
    },
    "bugs": {
        "url": "https://github.com/AntonAduisky/webpack-react-typescript.git/issues"
    },
    "scripts": {
        "start": "webpack serve --progress --mode development",
        "build": "yarn clean && webpack --progress --mode production",
        "lint": "yarn eslint src --ext .js,.ts,.tsx --ignore-pattern '!.*.js' --no-error-on-unmatched-pattern",
        "lint:fix": "yarn lint --fix",
        "clean": "rm -rf ./build"
    },
    "devDependencies": {
        "@babel/core": "^7.19.1",
        "@babel/preset-env": "^7.19.1",
        "@babel/preset-react": "^7.18.6",
        "@babel/preset-typescript": "^7.18.6",
        "@commitlint/cli": "^18.4.3",
        "@commitlint/config-conventional": "^18.4.3",
        "@svgr/webpack": "^8.1.0",
        "@types/node": "^20.9.3",
        "@types/react": "^18.2.38",
        "@types/react-dom": "^18.2.16",
        "@typescript-eslint/eslint-plugin": "^6.12.0",
        "@typescript-eslint/parser": "^6.12.0",
        "babel-loader": "^9.1.3",
        "css-loader": "^6.7.1",
        "eslint": "^8.54.0",
        "eslint-plugin-jsx-a11y": "^6.4.1",
        "eslint-plugin-prettier": "^5.0.1",
        "eslint-plugin-react": "^7.23.2",
        "eslint-plugin-react-hooks": "^4.2.0",
        "file-loader": "^6.2.0",
        "html-webpack-plugin": "^5.5.0",
        "husky": "^8.0.0",
        "lint-staged": "^15.1.0",
        "prettier": "^3.1.0",
        "style-loader": "^3.3.1",
        "ts-loader": "^9.4.1",
        "typescript": "^5.3.2",
        "webpack": "^5.74.0",
        "webpack-cli": "^5.1.4",
        "webpack-dev-server": "^4.11.1"
    },
    "dependencies": {
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-router": "^6.19.0",
        "react-router-dom": "^6.19.0"
    },
    "lint-staged": {
        "*.{js,jsx,ts,tsx}": [
            "eslint --quiet --cache --fix"
        ],
        "*.{json,js,ts,jsx,tsx,html}": [
            "prettier --write"
        ]
    },
    "license": "MIT",
    "private": true,
    "engines": {
        "node": ">=8.0.0",
        "npm": ">=6.0.0",
        "yarn": ">=1.0.0"
    }
}
